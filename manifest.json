{
    "manifest_version": 2,
    "name": "Acelera",
    "version":"0.0.1",
    "author": "Alessandra Julia",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions":[
        "activeTab"
    ]

}